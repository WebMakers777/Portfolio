{
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "vite",
    "outputDirectory": "dist",
    "env": {
        "NODE_ENV": "production"
    },
    "routes": [
        {
            "src": "/api/contact",
            "dest": "/api/contact.js"
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp|woff|woff2|eot|ttf|json)$",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}